{
  "name": "rrd108/vue-bake",
  "description": "CakePHP Plugin for Baking Vue Components",
  "version": "1.0.0",
  "type": "cakephp-plugin",
  "keywords": [
    "cakephp",
    "plugin",
    "vue"
  ],
  "license": "MIT",
  "require": {
    "cakephp/cakephp": "^5.0",
    "cakephp/bake": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.1"
  },
  "autoload": {
    "psr-4": {
      "VueBake\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VueBake\\Test\\": "tests/",
      "TestApp\\": "tests/test_app/src"
    }
  },
  "authors": [
    {
      "name": "rrd",
      "email": "rrd@webmania.cc"
    }
  ]
}
